module example

type A {
   A(i32)
}

effect state<a> {
   fun get() : a
   fun set(a) : unit
}

fun test() : state<i32> unit {
   val a = get<i32>()
   set(a+1)
}

fun test2(a:i32) : i32 {
   a + 1
}

fun test_inline() : unit {
   inline_python<unit>("print(\"test_inline\")")
}

fun main() : io unit {
  var a = 0
  val A(b) = A(2)
  var c = 0
  handle state<i32> {
     while c < 2 {
     test()
     test()
     test()
     c = c + 1
     }
  } with {
     fun get() {
        resume(a)
     }
     fun set(v:i32) {
        a = v
        resume(unit)
     }
  }
  if a < 10 {
     print(100)
  } else {
     print(1000)
  }
  while a < 10 {
    print(a)
    a = a + 1
  }
  print(test2(a))
  val f = fn () : i32 { a + 1 }
  f()
  print(b)
  test_inline()
  unit
}

