module data/list

fun reverse<a>(l : [a]) : [a] {
  var rest = l
  var newl = []
  while (rest != [] : [a]) {
    val e|r = rest
    rest = r
    newl = e|newl
  }
  newl
}

fun map<a, b>[e](l : [a], f : (a) -> e b) : e [b] {
  if l == [] : [a] {
    []:[b]
  } else {
    val e|r = l
    f(e)|map(r, f)
  }
}